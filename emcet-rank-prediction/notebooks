in\": 69284,\n        \"max\": 120608,\n        \"num_unique_values\": 10,\n        \"samples\": [\n          100514,\n          84396,\n          69284\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"Avg_Predicted_Score\",\n      \"properties\": {\n        \"dtype\": \"string\",\n        \"num_unique_values\": 10,\n        \"samples\": [\n          \"80.1%\",\n          \"82.17%\",\n          \"79.93%\"\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    }\n  ]\n}"}},"metadata":{}}]},{"cell_type":"code","source":["# --- STEP 1: LOAD ASSETS ---\n","import pandas as pd\n","import numpy as np\n","import joblib\n","import io\n","import warnings\n","from IPython.display import display, Markdown\n","\n","# Define paths (assuming these were set in your environment)\n","CUTOFF_DATA_PATH = '/content/drive/MyDrive/sisira/open_close_ranks.xlsx'\n","MODEL_SAVE_PATH = '/content/drive/MyDrive/sisira/best_rf_model.joblib'\n","COLUMNS_SAVE_PATH = '/content/drive/MyDrive/sisira/trained_columns.joblib'\n","\n","# Load Assets\n","try:\n","    best_model = joblib.load(MODEL_SAVE_PATH)\n","    X_train_cols = joblib.load(COLUMNS_SAVE_PATH)\n","    open_close_ranks = pd.read_excel(CUTOFF_DATA_PATH)\n","    open_close_ranks.columns = open_close_ranks.columns.str.lower().str.replace(' ', '_')\n","    open_close_ranks['max_rank'] = open_close_ranks[['opening_rank', 'closing_rank']].max(axis=1)\n","    open_close_ranks.rename(columns={'max_rank': 'closing_rank'}, inplace=True)\n","except Exception as e:\n","    display(Markdown(f\"FATAL ERROR: Could not load model or data. Error: {e}\"))\n","    raise\n","\n","# --- FINAL PREDICTION SCRIPT (CLEANEST OUTPUT) ---\n","\n","import pandas as pd\n","import joblib\n","import numpy as np\n","import warnings\n","import time\n","\n","# --- Configuration (Load paths should be set from your previous cells) ---\n","# Assuming these variables are still defined from Step 0/1:\n","# CUTOFF_DATA_PATH, MODEL_SAVE_PATH, COLUMNS_SAVE_PATH, X_train_cols, best_model, open_close_ranks\n","\n","# Load Assets (Re-running this block for reliability)\n","try:\n","    best_model = joblib.load(MODEL_SAVE_PATH)\n","    X_train_cols = joblib.load(COLUMNS_SAVE_PATH)\n","    open_close_ranks = pd.read_excel(CUTOFF_DATA_PATH)\n","    open_close_ranks.columns = open_close_ranks.columns.str.lower().str.replace(' ', '_')\n","    open_close_ranks['min_rank'] = open_close_ranks[['opening_rank', 'closing_rank']].min(axis=1)\n","    open_close_ranks['max_rank'] = open_close_ranks[['opening_rank', 'closing_rank']].max(axis=1)\n","    open_close_ranks.drop(columns=['opening_rank', 'closing_rank'], inplace=True)\n","    open_close_ranks.rename(columns={'min_rank': 'opening_rank', 'max_rank': 'closing_rank'}, inplace=True)\n","    open_close_ranks['max_rank'] = open_close_ranks['closing_rank'] # Restore max_rank for clarity\n","except Exception as e:\n","    print(f\"FATAL ERROR during asset loading: {e}. Check file paths and previous steps.\")\n","    raise\n","\n","# --- Filtered Prediction Logic (Using the function structure from the previous fix) ---\n","\n","def get_eligible_cutoffs(student_profile, cutoff_data):\n","    s_gender = student_profile['gender'].upper()\n","    s_category = student_profile['category'].upper()\n","    s_year = student_profile['year']\n","\n","    eligible_cutoffs = cutoff_data[cutoff_data['year'] == s_year].copy()\n","\n","    # Filter by Gender\n","    if s_gender == 'M':\n","        eligible_cutoffs = eligible_cutoffs[\n","            ~eligible_cutoffs['seat_category'].str.contains('GIRLS', case=False, na=False)\n","        ]\n","\n","    # Filter by Category (Must match category OR be Open Category)\n","    is_open = eligible_cutoffs['seat_category'].str.contains('OC|GEN|EWS', case=False, na=False, regex=True)\n","    is_specific = eligible_cutoffs['seat_category'].str.contains(s_category, case=False, na=False)\n","    eligible_cutoffs = eligible_cutoffs[is_open | is_specific].copy()\n","\n","    # Filter out Special Quotas\n","    special_quota_terms = ['PH', 'CAP', 'NCC', 'SP', 'MUS']\n","    for term in special_quota_terms:\n","        eligible_cutoffs = eligible_cutoffs[\n","            ~eligible_cutoffs['seat_category'].str.contains(term, case=False, na=False)\n","        ]\n","    return eligible_cutoffs\n","\n","def predict_clean_results(student_profile, model, cutoff_data, X_train_cols):\n","\n","    with warnings.catch_warnings():\n","        # Suppress the Data Frame fragmentation warnings\n","        warnings.simplefilter(\"ignore\", category=pd.errors.PerformanceWarning)\n","\n","        filtered_cutoffs = get_eligible_cutoffs(student_profile, cutoff_data)\n","        if filtered_cutoffs.empty:\n","            return pd.DataFrame()\n","\n","        # Prepare Student DF for cross-join\n","        student_df = pd.DataFrame([{\n","            'rank': student_profile['rank'],\n","            'gender': student_profile['gender'],\n","            'category': student_profile['category'],\n","            'rank_noisy': student_profile['rank']\n","        }])\n","\n","        # Build the final prediction DataFrame structure carefully\n","        num_scenarios = len(filtered_cutoffs)\n","        base_pred_data = {\n","            'rank_noisy': np.repeat(student_df['rank_noisy'].iloc[0], num_scenarios),\n","            'gender': np.repeat(student_df['gender'].iloc[0], num_scenarios),\n","            'category': np.repeat(student_df['category'].iloc[0], num_scenarios),\n","            'college': filtered_cutoffs['college'].values,\n","            'branch': filtered_cutoffs['branch'].values,\n","            'seat_category': filtered_cutoffs['seat_category'].values,\n","            'opening_rank': filtered_cutoffs['opening_rank'].values,\n","            'closing_rank': filtered_cutoffs['closing_rank'].values,\n","        }\n","        prediction_df = pd.DataFrame(base_pred_data)\n","\n","        # Apply Feature Engineering\n","        prediction_df['rank_difference'] = prediction_df['rank_noisy'] - prediction_df['closing_rank']\n","        prediction_df['rank_vs_opening'] = prediction_df['rank_noisy'] - prediction_df['opening_rank']\n","\n","        X_pred = prediction_df.drop(columns=['opening_rank', 'closing_rank', 'rank_noisy', 'college', 'branch', 'seat_category'])\n","\n","        # One-Hot Encoding and Alignment\n","        categorical_cols_pred = X_pred.select_dtypes(include=['object']).columns.tolist()\n","        X_pred = pd.get_dummies(X_pred, columns=categorical_cols_pred, drop_first=True)\n","\n","        missing_cols = set(X_train_cols) - set(X_pred.columns)\n","        for c in missing_cols:\n","            X_pred[c] = 0\n","\n","        X_pred = X_pred.reindex(columns=X_train_cols, fill_value=0)\n","\n","        # Predict Probabilities\n","        probabilities = model.predict_proba(X_pred)[:, 1]\n","\n","        # Final clean aggregation\n","        prediction_df['Probability'] = probabilities\n","        results = prediction_df.groupby(['college', 'branch'])['Probability'].max().reset_index()\n","\n","        results.rename(columns={'Probability': 'Max_Probability'}, inplace=True)\n","        results['Max_Probability'] = (results['Max_Probability'] * 100).round(2)\n","        results = results.sort_values('Max_Probability', ascending=False)\n","\n","    return results\n","\n","\n","# --- EXECUTION ---\n","\n","# Define the inputs you requested (Rank, Gender, Caste)\n","# Note: We must still provide a 'year' internally to look up cutoffs, so 2024 is used by default.\n","# --- 0. GET USER INPUT ---\n","rank_value = int(input(\"Enter your rank: \"))\n","gender_input = input(\"Enter your gender (M/F): \").strip().upper()\n","category_input = input(\"Enter your category (OC/BC/SC/ST): \").strip().upper()\n","\n","# Store in test_profile dictionary for internal use\n","test_profile = {\n","    'rank': rank_value,\n","    'gender': gender_input,\n","    'category': category_input,\n","    'year': 2024  # You can keep the latest year from cutoff table if needed\n","}\n","\n","# Execute prediction\n","final_results = predict_clean_results(\n","    student_profile=test_profile,\n","    model=best_model,\n","    cutoff_data=open_close_ranks,\n","    X_train_cols=X_train_cols\n",")\n","\n","print(\"\\n=======================================================\")\n","print(f\"PREDICTION RESULTS for Rank {test_profile['rank']} ({test_profile['category']}, {test_profile['gender']})\")\n","print(\"=======================================================\")\n","\n","if not final_results.empty:\n","    # Use 70% as the threshold for strong recommendations\n","    high_prob_results = final_results[final_results['Max_Probability'] >= 70.0].head(15)\n","\n","    if not high_prob_results.empty:\n","        print(\"\\n‚úÖ TOP RECOMMENDED COLLEGES (70%+ Chance):\")\n","        print(\"----------------------------------------\")\n","        print(high_prob_results.to_string(index=False))\n","\n","        # Show next tier if available\n","        next_best = final_results[final_results['Max_Probability'] < 70.0].head(5)\n","        if not next_best.empty:\n","            print(\"\\n--- Next Best Options (< 70% Chance) ---\")\n","            print(next_best.to_string(index=False))\n","    else:\n","        print(\"\\n‚û°Ô∏è TOP 15 OPTIONS (All Probabilities):\")\n","        print(\"---------------------------------\")\n","        print(final_results.head(15).to_string(index=False))\n","else:\n","    print(\"No eligible colleges found for this profile after applying filters.\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"dig-Vmbvlo_o","executionInfo":{"status":"ok","timestamp":1761676006530,"user_tz":-330,"elapsed":60129,"user":{"displayName":"Pawan Kalyan JPK","userId":"17512457818931320333"}},"outputId":"ab8bf3a5-b61b-47f2-a205-715147d403c8"},"execution_count":null,"outputs":[